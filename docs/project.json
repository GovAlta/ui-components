{
  "name": "docs",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "docs/src",
  "implicitDependencies": [
    "react-components",
    "common"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/docs"],
      "options": {
        "command": "astro build --root docs"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "astro dev --root docs --port 4203 --host 0.0.0.0"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "astro preview --root docs --port 4304 --host 0.0.0.0"
      }
    }
  },
  "tags": []
}
