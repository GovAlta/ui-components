{
  "name": "goa-web-components",
  "description": "A base template for building a shareable web components library with Svelte",
  "version": "1.0.0",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "keywords": [
    "svelte",
    "web-components",
    "library"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:demo": "rollup -c rollup.demo.config.js",
    "dev": "rollup -c rollup.demo.config.js -w",
    "start": "sirv demo/public",
    "prepublishOnly": "npm run build",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "rollup": "^2.0.0",
    "rollup-plugin-clean": "^1.0.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "files": [
    "dist"
  ]
}
