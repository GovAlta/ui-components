{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "lib": [
      "es2017",
      "dom"
    ],
    "allowJs": true,
    "strict": true,
    "noImplicitAny": false,
    "types": [
      "cypress",
      "node",
      "cypress-axe",
      "cypress-file-upload"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "**/*.ts"
  ]
}